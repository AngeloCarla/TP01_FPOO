int num1, num2;
int suma, resta, multiplicacion, division;

public void setup(){
 num1 = 10;
 num2 = 5;
 
 suma = num1 + num2;
 println("Suma:", num1, "+", num2, "=", suma);
 
 resta = num1 - num2;
 println("Resta:", num1, "-", num2, "=", resta);
 
 multiplicacion = num1 * num2;
 println("Multiplicacion:", num1, "*", num2, "=", multiplicacion);
 
 division = num1 / num2;
 println("Division:", num1, "/", num2, "=", division);
}
